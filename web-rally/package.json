    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.12.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "form-data": "^4.0.4",
    "jsdom": "^27.0.1",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.553.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
